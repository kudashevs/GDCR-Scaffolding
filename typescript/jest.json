{
  "verbose": true,
  "testRegex": "(\\.test)\\.(ts|tsx|js)$",
  "moduleFileExtensions": [
    "ts",
    "js"
  ],
  "transform": {
    "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
  },
  "testPathIgnorePatterns": [
    "/node_modeles/",
    "/build/"
  ],
  "testEnvironment": "node"
}
